{
  "name": "mui-language-picker",
  "version": "1.2.3",
  "description": "Material UI language picker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist src/index coverage",
    "index": "node src/langPicker/makeIndexes.js",
    "build": "npm run index && npm test && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.{ts.tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sillsdev/mui-language-picker.git"
  },
  "keywords": [
    "language",
    "bcp47",
    "font",
    "script"
  ],
  "author": "Greg Trihus <greg_trihus@sil.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sillsdev/mui-language-picker/issues"
  },
  "homepage": "https://github.com/sillsdev/mui-language-picker#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.162",
    "@types/node": "^16.11.52",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "copyfiles": "^2.4.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-testing-library": "^8.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "write": "^2.0.0"
  },
  "peerdependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|.*\\.test)\\.(ts|tsx)$"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
